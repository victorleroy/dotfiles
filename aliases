# Get External IP / local IPs
alias ip=“curl ipinfo.io/ip”
alias ips=“ifconfig -a | perl -nle’/(\d+\.\d+\.\d+\.\d+)/ && print $1'”
alias speedtest=“wget -O /dev/null http://speedtest.wdc01.softlayer.com/downloads/test10.zip”

# Quickly serve the current directory as HTTP
alias serve=‘ruby -run -e httpd . -p 8000’  # Or python -m SimpleHTTPServer :slightly_smiling_face:

# Rails server
alias s=“rails s”

# Redis server
alias r=“redis-server”

# Rails console
alias c=“rails console”

# Skello folders path
alias sk=“/Users/victorleroy/code/Skello/skello”
alias skm=“/Users/victorleroy/code/Skello/skellomobile”
alias skb=“/Users/victorleroy/code/Skello/skello_timekeeper”

alias ga=‘git add .’
